{
  "expo": {
    "name": "lurdinha-app",
    "slug": "lurdinha-app",
    "version": "1.0.1",
    "orientation": "portrait",
    "icon": "./assets/logo.png",
    "userInterfaceStyle": "dark",
    "newArchEnabled": true,
    "splash": {
      "image": "./assets/splash-icon.png",
      "resizeMode": "contain",
      "backgroundColor": "#000000"
    },
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.lurdinha.app",
      "buildNumber": "2",
      "icon": "./assets/app-store-icon-1024.png",
      "infoPlist": {
        "NSCameraUsageDescription": "Precisamos acessar sua câmera para você tirar fotos de perfil.",
        "NSPhotoLibraryUsageDescription": "Precisamos acessar suas fotos para você escolher uma foto de perfil.",
        "NSPhotoLibraryAddUsageDescription": "Precisamos salvar fotos de perfil na sua biblioteca de fotos.",
        "ITSAppUsesNonExemptEncryption": false
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#000000"
      },
      "edgeToEdgeEnabled": true,
      "package": "com.lurdinha.app",
      "versionCode": 1
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "plugins": [
      "expo-apple-authentication"
    ],
    "extra": {
      "eas": {
        "projectId": "279141d4-e633-4c9d-adfc-39f8733c4798"
      }
    }
  }
}